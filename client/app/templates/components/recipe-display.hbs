<div class="col-md-3">
    <div class="panel panel-default" property="{{id}}">
        <div class="panel-heading">
            <h3 class="panel-title">{{name}}</h3>
        </div>
        <div class="panel-body">
            <p><img src="{{image}}"/></p>
            <p>{{body}}</p>
        </div>
        <div class="panel-footer">Matching tags: {{tags}}</div>
    </div>
</div>